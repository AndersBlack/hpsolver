(define
	(problem zenotravel04)
	(:domain  zenotravel)
	(:objects
		c1 - City
		c2 - City
		c3 - City
		a1 - Aircraft
		a2 - Aircraft
		p4 - Person
		p1 - Person
		p5 - Person
		p3 - Person
		p2 - Person
		f6 - FLevel
		f0 - FLevel
		f4 - FLevel
		f3 - FLevel
		f5 - FLevel
		f2 - FLevel
		f1 - FLevel
	)
	(:htn
		:parameters ()
		:subtasks (and
		 (task0 (transport-person p2 c3))
		 (task1 (transport-person p3 c1))
		 (task2 (transport-person p4 c2))
		 (task3 (transport-person p5 c3))
		 (task4 (transport-aircraft a1 c1))
		)
		:ordering (and
			(< task1 task0)
			(< task2 task1)
			(< task0 task3)
			(< task3 task4)
		)
	)
	(:init
		(next f0 f1)
		(next f1 f2)
		(next f2 f3)
		(next f3 f4)
		(next f4 f5)
		(next f5 f6)
		(next f5 f6)
		(different c1 c2)
		(different c1 c3)
		(different c2 c1)
		(different c2 c3)
		(different c3 c1)
		(different c3 c2)
		(at a1 c3)
		(at a2 c3)
		(at p1 c1)
		(at p2 c2)
		(at p3 c1)
		(at p4 c1)
		(at p5 c3)
		(fuel-level a1 f5)
		(fuel-level a2 f0)
	)
	(:goal
		(and
		(at a1 c1)
		(at p2 c3)
		(at p3 c1)
		(at p4 c2)
		(at p5 c3)
		)
	)
)
