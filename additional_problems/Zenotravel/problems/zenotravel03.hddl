(define
	(problem zenotravel03)
	(:domain  zenotravel)
	(:objects
		c1 - City
		c2 - City
		c3 - City
		a1 - Aircraft
		a2 - Aircraft
		p1 - Person
		p2 - Person
		p3 - Person
		p4 - Person
		f6 - FLevel
		f0 - FLevel
		f4 - FLevel
		f3 - FLevel
		f5 - FLevel
		f2 - FLevel
		f1 - FLevel
	)
	(:htn
		:parameters ()
		:subtasks (and
		 (task0 (transport-person p1 c2))
		 (task1 (transport-person p2 c1))
		 (task2 (transport-person p3 c1))
		 (task3 (transport-person p4 c2))
		 (task4 (transport-person p2 c2))
		 (task5 (transport-aircraft a2 c3))
		)
		:ordering (and
			(< task5 task0)
			(< task3 task2)
			(< task4 task3)
			(< task1 task4)
			(< task2 task5)
		)
	)
	(:init
		(next f0 f1)
		(next f1 f2)
		(next f2 f3)
		(next f3 f4)
		(next f4 f5)
		(next f5 f6)
		(next f5 f6)
		(different c1 c2)
		(different c1 c3)
		(different c2 c1)
		(different c2 c3)
		(different c3 c1)
		(different c3 c2)
		(at a1 c1)
		(at a2 c3)
		(at p1 c1)
		(at p2 c1)
		(at p3 c2)
		(at p4 c2)
		(fuel-level a1 f4)
		(fuel-level a2 f5)
	)
	(:goal
		(and
		(at a2 c3)
		(at p1 c2)
		(at p2 c2)
		(at p3 c1)
		(at p4 c2)
		)
	)
)
